# Web App Configuration
VITE_APP_NAME=ContentAutoPilot
VITE_APP_VERSION=1.0.0
VITE_NODE_ENV=development

# Server Configuration
VITE_WEB_HOST=localhost
VITE_WEB_PORT=5173
